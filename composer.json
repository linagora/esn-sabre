{
    "name": "linagora/esn-sabre",
    "version": "1.11.0-dev",
    "description": "SabreDAV Backend for Linagora ESN",
    "keywords": ["WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "license" : "AGPL-3.0",
    "authors": [{
            "name": "Linagora",
            "homepage" : "http://linagora.com"
        },{
            "name": "Philipp Kewisch",
            "email": "pkewisch@linagora.com",
            "role" : "Initiator"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/linagora/sabre-vobject.git",
            "no-api": true
        }
    ],
    "config": {
        "preferred-install": "dist",
        "github-protocols": ["https"]
    },
    "require": {
        "php-amqplib/php-amqplib": "^3.3",
        "php": ">=8.4",
        "sabre/dav": "4.7.0",
        "sabre/vobject": "dev-linagora-waiting-merges-4.5#df65b241c18c4c08cca31fb4dc71b82d231dfc06 as 4.5.7",
        "mongodb/mongodb": "^2.4",
        "monolog/monolog": "^2.9",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^12.0",
        "squizlabs/php_codesniffer": "3.10.3"
    },
    "autoload": {
        "psr-4" : {
            "ESN\\DAV\\"        : "lib/DAV/",
            "ESN\\CalDAV\\"     : "lib/CalDAV/",
            "ESN\\CardDAV\\"    : "lib/CardDAV/",
            "ESN\\DAVACL\\"     : "lib/DAVACL/",
            "ESN\\JSON\\"       : "lib/JSON/",
            "ESN\\Publisher\\"  : "lib/Publisher/",
            "ESN\\Utils\\"      : "lib/Utils/",
            "ESN\\Log\\"        : "lib/Log/"
        }
    },
    "minimum-stability": "dev"
}
