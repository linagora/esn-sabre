{
    "name": "linagora/esn-sabre",
    "version": "1.11.0-dev",
    "description": "SabreDAV Backend for Linagora ESN",
    "keywords": ["WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "license" : "AGPL-3.0",
    "authors": [{
            "name": "Linagora",
            "homepage" : "http://linagora.com"
        },{
            "name": "Philipp Kewisch",
            "email": "pkewisch@linagora.com",
            "role" : "Initiator"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/bastien-roucaries/vobject"
        }
    ],
    "require": {
        "php-amqplib/php-amqplib": "^3.3",
        "php": ">=7.4",
        "sabre/dav": "4.1.5",
        "sabre/vobject": "dev-waiting-merges-4.2.2 as 4.2.2",
        "mongodb/mongodb": "^1.15",
        "monolog/monolog": "^2.9",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^9.0",
        "squizlabs/php_codesniffer": "3.8.0"
    },
    "autoload": {
        "psr-4" : {
            "ESN\\DAV\\"        : "lib/DAV/",
            "ESN\\CalDAV\\"     : "lib/CalDAV/",
            "ESN\\CardDAV\\"    : "lib/CardDAV/",
            "ESN\\DAVACL\\"     : "lib/DAVACL/",
            "ESN\\JSON\\"       : "lib/JSON/",
            "ESN\\Publisher\\"  : "lib/Publisher/",
            "ESN\\Utils\\"      : "lib/Utils/",
            "ESN\\Log\\"        : "lib/Log/"
        }
    },
    "minimum-stability": "dev"
}
